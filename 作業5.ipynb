    "# 模擬購物車數量的變化\n",
    "cart_quantity = 0  # 初始購物車數量\n",
    "\n",
    "def add_to_cart():\n",
    "    global cart_quantity\n",
    "    cart_quantity += 1  # 每次點擊按鈕，購物車數量增加 1\n",
    "    print(f\"目前購物車數量: {cart_quantity}\")\n",
    "\n",
    "# 模擬點擊按鈕兩次\n",
    "add_to_cart()  # 第一次點擊\n",
    "add_to_cart()  # 第二次點擊\n"
   ]
